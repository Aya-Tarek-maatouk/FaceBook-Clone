@model IEnumerable<Facebook.Models.ApplicationUser>
<!-- Modal -->

                <table class="table">
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                <form class="row">
                                    <img src="~/Image/@item.image" style="width:80px;height:60px;border-radius:100%;border:5px solid white;" />
                                    @Html.ActionLink(item.UserName, "ShowFriendProfile", "User",routeValues: new{ FriendId = item.Id },htmlAttributes:new {@class="" })
                                </form>
                            </td>
                        </tr>
                    }
                </table>
